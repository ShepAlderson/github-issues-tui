SortOrder\n   353→\t\t\t\tif err := config.Save(cfg); err != nil {\n   354→\t\t\t\t\t_ = fmt.Errorf(\"failed to save config: %w\", err)\n   355→\t\t\t\t}\n   356→\t\t\t\tdisplayIssues()\n   357→\t\t\t\treturn nil\n   358→\t\t\tcase 'm', 'M':\n   359→\t\t\t\t// Toggle markdown rendering\n   360→\t\t\t\tmarkdownRendered = !markdownRendered\n   361→\t\t\t\tupdateStatusBar()\n   362→\t\t\t\t// Refresh detail view with current issue\n   363→\t\t\t\tif currentIndex >= 0 && currentIndex < len(issues) {\n   364→\t\t\t\t\tissueNum := issues[currentIndex].Number\n   365→\t\t\t\t\tdetail, err := db.GetIssueDetail(database, owner, repo, issueNum)\n   366→\t\t\t\t\tif err == nil && detail != nil {\n   367→\t\t\t\t\t\tdetailView.SetText(formatIssueDetailFull(detail))\n   368→\t\t\t\t\t}\n   369→\t\t\t\t}\n   370→\t\t\t\treturn nil\n   371→\t\t\tcase 'r', 'R':\n   372→\t\t\t\t// Manual refresh - only if not already refreshing and not in comments view\n   373→\t\t\t\tif !isRefreshing && !inCommentsView {\n   374→\t\t\t\t\tisRefreshing = true\n   375→\t\t\t\t\tminorError = \"\" // Clear previous minor error\n   376→\t\t\t\t\tupdateStatusBar()\n   377→\n   378→\t\t\t\t\t// Run refresh in a goroutine\n   379→\t\t\t\t\tgo func() {\n   380→\t\t\t\t\t\tprogress := func(current, total int, status string) {\n   381→\t\t\t\t\t\t\trefreshStatus = status\n   382→\t\t\t\t\t\t\tapp.QueueUpdateDraw(func() {\n   383→\t\t\t\t\t\t\t\tupdateStatusBar()\n   384→\t\t\t\t\t\t\t})\n   385→\t\t\t\t\t\t}\n   386→\n   387→\t\t\t\t\t\terr := RefreshSync(dbPath, cfg, progress)\n   388→\n   389→\t\t\t\t\t\tapp.QueueUpdateDraw(func() {\n   390→\t\t\t\t\t\t\tisRefreshing = false\n   391→\t\t\t\t\t\t\trefreshStatus = \"\"\n   392→\t\t\t\t\t\t\tif err != nil {\n   393→\t\t\t\t\t\t\t\tuiErr := errors.NewUIError(err)\n   394→\t\t\t\t\t\t\t\tif uiErr.Category == errors.CategoryCritical {\n   395→\t\t\t\t\t\t\t\t\t// Show critical error as modal\n   396→\t\t\t\t\t\t\t\t\tshowCriticalErrorModal(app, pages, uiErr)\n   397→\t\t\t\t\t\t\t\t} else {\n   398→\t\t\t\t\t\t\t\t\t// Show minor error in status bar\n   399→\t\t\t\t\t\t\t\t\tminorError = uiErr.Hint.Message\n   400→\t\t\t\t\t\t\t\t}\n   401→\t\t\t\t\t\t\t}\n   402→\t\t\t\t\t\t\tupdateStatusBar()\n   403→\t\t\t\t\t\t\t// Refresh the issue list\n   404→\t\t\t\t\t\t\tdisplayIssues()\n   405→\t\t\t\t\t\t})\n   406→\t\t\t\t\t}()\n   407→\t\t\t\t} else if minorError != \"\" {\n   408→\t\t\t\t\t// If there's a minor error, pressing 'r' clears it and retries\n   409→\t\t\t\t\tminorError = \"\"\n   410→\t\t\t\t\t// Trigger refresh\n   411→\t\t\t\t\tisRefreshing = true\n   412→\t\t\t\t\tupdateStatusBar()\n   413→\n   414→\t\t\t\t\tgo func() {\n   415→\t\t\t\t\t\tprogress := func(current, total int, status string) {\n   416→\t\t\t\t\t\t\trefreshStatus = status\n   417→\t\t\t\t\t\t\tapp.QueueUpdateDraw(func() {\n   418→\t\t\t\t\t\t\t\tupdateStatusBar()\n   419→\t\t\t\t\t\t\t})\n   420→\t\t\t\t\t\t}\n   421→\n   422→\t\t\t\t\t\terr := RefreshSync(dbPath, cfg, progress)\n   423→\n   424→\t\t\t\t\t\tapp.QueueUpdateDraw(func() {\n   425→\t\t\t\t\t\t\tisRefreshing = false\n   426→\t\t\t\t\t\t\trefreshStatus = \"\"\n   427→\t\t\t\t\t\t\tif err != nil {\n   428→\t\t\t\t\t\t\t\tuiErr := errors.NewUIError(err)\n   429→\t\t\t\t\t\t\t\tif uiErr.Category == errors.CategoryCritical {\n   430→\t\t\t\t\t\t\t\t\tshowCriticalErrorModal(app, pages, uiErr)\n   431→\t\t\t\t\t\t\t\t} else {\n   432→\t\t\t\t\t\t\t\t\tminorError = uiErr.Hint.Message\n   433→\t\t\t\t\t\t\t\t}\n   434→\t\t\t\t\t\t\t}\n   435→\t\t\t\t\t\t\tupdateStatusBar()\n   436→\t\t\t\t\t\t\tdisplayIssues()\n   437→\t\t\t\t\t\t})\n   438→\t\t\t\t\t}()\n   439→\t\t\t\t}\n   440→\t\t\t\treturn nil\n   441→\t\t\t}\n   442→\t\tcase tcell.KeyEscape:\n   443→\t\t\t// Dismiss modal if visible, or return from comments view\n   444→\t\t\tif pages.HasPage(\"help\") {\n   445→\t\t\t\tpages.SwitchToPage(\"main\")\n   446→\t\t\t\tapp.SetFocus(app.GetFocus())\n   447→\t\t\t} else if pages.HasPage(\"error\") {\n   448→\t\t\t\tpages.SwitchToPage(\"main\")\n   449→\t\t\t\tapp.SetFocus(app.GetFocus())\n   450→\t\t\t} else if pages.HasPage(\"comments\") {\n   451→\t\t\t\treturnToIssueList()\n   452→\t\t\t}\n   453→\t\t\treturn event\n   454→\t\tcase tcell.KeyCtrlC:\n   455→\t\t\tapp.Stop()\n   456→\t\t\treturn nil\n   457→\t\t}\n   458→\t\treturn event\n   459→\t})\n   460→\n   461→\t// Initial load of issues\n   462→\tdisplayIssues()\n   463→\n   464→\t// Update detail view when selection changes\n   465→\tissueList.SetChangedFunc(func(index int, mainText, secondaryText string, shortcut rune) {\n   466→\t\tcurrentIndex = index\n   467→\t\tif index >= 0 && index < len(issues) {\n   468→\t\t\tissueNum := issues[index].Number\n   469→\t\t\tdetail, err := db.GetIssueDetail(database, owner, repo, issueNum)\n   470→\t\t\tif err == nil && detail != nil {\n   471→\t\t\t\tdetailView.SetText(formatIssueDetailFull(detail))\n   472→\t\t\t}\n   473→\t\t}\n   474→\t})\n   475→\n   476→\t// Update detail view and show comments on selection confirm\n   477→\tissueList.SetSelectedFunc(func(index int, mainText, secondaryText string, shortcut rune) {\n   478→\t\tcurrentIndex = index\n   479→\t\tif index >= 0 && index < len(issues) {\n   480→\t\t\tissueNum := issues[index].Number\n   481→\t\t\tdetail, err := db.GetIssueDetail(database, owner, repo, issueNum)\n   482→\t\t\tif err == nil && detail != nil {\n   483→\t\t\t\tdetailView.SetText(formatIssueDetailFull(detail))\n   484→\t\t\t}\n   485→\t\t\t// Show comments view\n   486→\t\t\tshowComments(issueNum)\n   487→\t\t}\n   488→\t})\n   489→\n   490→\t// Add header\n   491→\theader := tview.NewTextView()\n   492→\theader.SetText(fmt.Sprintf(\" ghissues - %s/%s \", owner, repo))\n   493→\theader.SetTextAlign(tview.AlignCenter)\n   494→\n   495→\t// Create main layout with vertical split\n   496→\tflex := tview.NewFlex()\n   497→\tflex.SetDirection(tview.FlexColumn)\n   498→\tflex.AddItem(issueList, 0, 1, true)\n   499→\tflex.AddItem(detailView, 0, 2, false)\n   500→\n   501→\tpages.AddPage(\"main\", flex, true, true)\n   502→\n   503→\t// Main layout with header\n   504→\tmainFlex := tview.NewFlex()\n   505→\tmainFlex.SetDirection(tview.FlexRow)\n   506→\tmainFlex.AddItem(header, 1, 0, false)\n   507→\tmainFlex.AddItem(pages, 0, 1, true)\n   508→\tmainFlex.AddItem(statusBar, 1, 0, false)\n   509→\n   510→\tapp.SetRoot(mainFlex, true)\n   511→\tapp.SetFocus(issueList)\n   512→\n   513→\treturn app.Run()\n   514→}\n   515→\n   516→// formatIssueForDisplay formats the main text of an issue for the list view\n   517→func formatIssueForDisplay(issue db.IssueList, columns []string) string {\n   518→\tvar parts []string\n   519→\tfor _, col := range columns {\n   520→\t\tswitch col {\n   521→\t\tcase \"number\":\n   522→\t\t\tparts = append(parts, fmt.Sprintf(\"#%d\", issue.Number))\n   523→\t\tcase \"title\":\n   524→\t\t\tparts = append(parts, issue.Title)\n   525→\t\tcase \"author\":\n   526→\t\t\tparts = append(parts, issue.Author)\n   527→\t\tcase \"date\":\n   528→\t\t\tparts = append(parts, formatDate(issue.CreatedAt))\n   529→\t\tcase \"comments\":\n   530→\t\t\tif issue.CommentCnt > 0 {\n   531→\t\t\t\tparts = append(parts, fmt.Sprintf(\"%d comments\", issue.CommentCnt))\n   532→\t\t\t}\n   533→\t\t}\n   534→\t}\n   535→\treturn strings.Join(parts, \" | \")\n   536→}\n   537→\n   538→// formatIssueSecondary formats the secondary text of an issue\n   539→func formatIssueSecondary(issue db.IssueList, columns []string) string {\n   540→\tvar parts []string\n   541→\tfor _, col := range columns {\n   542→\t\tif col == \"number\" || col == \"title\" {\n   543→\t\t\tcontinue\n   544→\t\t}\n   545→\t\tswitch col {\n   546→\t\tcase \"author\":\n   547→\t\t\tparts = append(parts, \"by \"+issue.Author)\n   548→\t\tcase \"date\":\n   549→\t\t\tparts = append(parts, formatDate(issue.CreatedAt))\n   550→\t\tcase \"comments\":\n   551→\t\t\tif issue.CommentCnt > 0 {\n   552→\t\t\t\tparts = append(parts, fmt.Sprintf(\"%d comments\", issue.CommentCnt))\n   553→\t\t\t}\n   554→\t\t}\n   555→\t}\n   556→\treturn strings.Join(parts, \" \")\n   557→}\n   558→\n   559→// formatIssueDetail formats an issue for the detail view (for backward compatibility with tests)\n   560→func formatIssueDetail(issue db.IssueList, owner, repo string) string {\n   561→\treturn fmt.Sprintf(`\n   562→ Issue #%d - %s\n   563→\n   564→ State: %s\n   565→ Author: %s\n   566→ Created: %s\n   567→ Comments: %d\n   568→\n   569→ URL: https://github.com/%s/%s/issues/%d\n   570→`,\n   571→\t\tissue.Number,\n   572→\t\tissue.Title,\n   573→\t\tissue.State,\n   574→\t\tissue.Author,\n   575→\t\tformatDate(issue.CreatedAt),\n   576→\t\tissue.CommentCnt,\n   577→\t\towner, repo,\n   578→\t\tissue.Number,\n   579→\t)\n   580→}\n   581→\n   582→// formatDate formats a date string for display\n   583→func formatDate(dateStr string) string {\n   584→\t// Try to parse and format the date nicely\n   585→\t// The date comes from database as RFC3339 format\n   586→\t// For now, just return it as-is or a simple version\n   587→\tif len(dateStr) >= 10 {\n   588→\t\treturn dateStr[:10]\n   589→\t}\n   590→\treturn dateStr\n   591→}\n   592→\n   593→// FormatRelativeTime formats a timestamp as a relative time string\n   594→// e.g., \"5 minutes ago\", \"2 hours ago\", \"3 days ago\"\n   595→// Returns \"never\" for empty or invalid timestamps\n   596→func FormatRelativeTime(timestamp string) string {\n   597→\tif timestamp == \"\" {\n   598→\t\treturn \"never\"\n   599→\t}\n   600→\n   601→\t// Try to parse the timestamp\n   602→\tt, err := time.Parse(time.RFC3339, timestamp)\n   603→\tif err != nil {\n   604→\t\treturn \"never\"\n   605→\t}\n   606→\n   607→\tnow := time.Now()\n   608→\tdiff := now.Sub(t)\n   609→\n   610→\t// Handle future timestamps (show as \"just now\")\n   611→\tif diff < 0 {\n   612→\t\treturn \"just now\"\n   613→\t}\n   614→\n   615→\t// Just now (less than 1 minute)\n   616→\tif diff < time.Minute {\n   617→\t\treturn \"just now\"\n   618→\t}\n   619→\n   620→\t// Minutes\n   621→\tif diff < time.Hour {\n   622→\t\tmins := int(diff.Minutes())\n   623→\t\tif mins == 1 {\n   624→\t\t\treturn \"1 minute ago\"\n   625→\t\t}\n   626→\t\treturn fmt.Sprintf(\"%d minutes ago\", mins)\n   627→\t}\n   628→\n   629→\t// Hours\n   630→\tif diff < 24*time.Hour {\n   631→\t\thours := int(diff.Hours())\n   632→\t\tif hours == 1 {\n   633→\t\t\treturn \"1 hour ago\"\n   634→\t\t}\n   635→\t\treturn fmt.Sprintf(\"%d hours ago\", hours)\n   636→\t}\n   637→\n   638→\t// Days\n   639→\tdays := int(diff.Hours() / 24)\n   640→\tif days == 1 {\n   641→\t\treturn \"1 day ago\"\n   642→\t}\n   643→\treturn fmt.Sprintf(\"%d days ago\", days)\n   644→}\n   645→\n   646→// GetLastSyncedDisplay returns a formatted string for the status bar\n   647→// showing when the data was last synced\n   648→func GetLastSyncedDisplay(timestamp string) string {\n   649→\treturn fmt.Sprintf(\"Last synced: %s\", FormatRelativeTime(timestamp))\n   650→}\n   651→\n   652→// showHelp shows a help modal\n   653→func showHelp(app *tview.Application, pages *tview.Pages) {\n   654→\thelpText := `\n   655→ Keyboard Shortcuts\n   656→\n   657→ Navigation:\n   658→   j / Down Arrow  - Move down\n   659→   k / Up Arrow    - Move up\n   660→   g / Home       - Go to first item\n   661→   G / End        - Go to last item\n   662→\n   663→ Actions:\n   664→   Enter          - View comments for selected issue\n   665→   r              - Refresh issues from GitHub\n   666→\n   667→ View:\n   668→   m              - Toggle markdown rendered/raw\n   669→\n   670→ Comments View:\n   671→   m              - Toggle markdown rendered/raw\n   672→   q / Esc        - Return to issue list\n   673→\n   674→ Sorting:\n   675→   s              - Cycle sort (updated → created → number → comments)\n   676→   S              - Reverse sort order\n   677→\n   678→ Other:\n   679→   ?              - Show this help\n   680→   q / Esc        - Quit help / Quit application\n   681→   Ctrl+C         - Force quit\n   682→\n   683→`\n   684→\thelpView := tview.NewModal()\n   685→\thelpView.SetText(helpText)\n   686→\thelpView.AddButtons([]string{\"Close\"})\n   687→\thelpView.SetDoneFunc(func(buttonIndex int, buttonLabel string) {\n   688→\t\tpages.SwitchToPage(\"main\")\n   689→\t\tapp.SetFocus(app.GetFocus())\n   690→\t})\n   691→\n   692→\tpages.AddPage(\"help\", helpView, true, true)\n   693→\tpages.SwitchToPage(\"help\")\n   694→}\n   695→\n   696→// ParseColumns parses a comma-separated list of column names\n   697→func ParseColumns(s string) []string {\n   698→\tif s == \"\" {\n   699→\t\treturn config.DefaultColumns()\n   700→\t}\n   701→\tparts := strings.Split(s, \",\")\n   702→\tvar columns []string\n   703→\tfor _, p := range parts {\n   704→\t\tp = strings.TrimSpace(p)\n   705→\t\tif p != \"\" {\n   706→\t\t\tcolumns = append(columns, p)\n   707→\t\t}\n   708→\t}\n   709→\tif len(columns) == 0 {\n   710→\t\treturn config.DefaultColumns()\n   711→\t}\n   712→\treturn columns\n   713→}\n   714→\n   715→// ColumnsToString converts a column slice to a comma-separated string\n   716→func ColumnsToString(columns []string) string {\n   717→\treturn strings.Join(columns, \",\")\n   718→}\n   719→\n   720→// ValidateColumns validates that all column names are valid\n   721→func ValidateColumns(columns []string) bool {\n   722→\tif len(columns) == 0 {\n   723→\t\treturn false\n   724→\t}\n   725→\tvalidColumns := map[string]bool{\n   726→\t\t\"number\":   true,\n   727→\t\t\"title\":    true,\n   728→\t\t\"author\":   true,\n   729→\t\t\"date\":     true,\n   730→\t\t\"comments\": true,\n   731→\t}\n   732→\tfor _, col := range columns {\n   733→\t\tif !validColumns[col] {\n   734→\t\t\treturn false\n   735→\t\t}\n   736→\t}\n   737→\treturn true\n   738→}\n   739→\n   740→// GetColumnIndex returns the index of a column in the list, or -1 if not found\n   741→func GetColumnIndex(columns []string, n