ling\n\n<promise>COMPLETE</promise>"}],"stop_reason":null,"stop_sequence":null,"usage":{"input_tokens":0,"cache_creation_input_tokens":116,"cache_read_input_tokens":83899,"cache_creation":{"ephemeral_5m_input_tokens":116,"ephemeral_1h_input_tokens":0},"output_tokens":6,"service_tier":"standard"},"context_management":null},"parent_tool_use_id":null,"session_id":"cf087fba-f80b-48a4-8fa5-bd895a9a115e","uuid":"b9ae2a29-8f83-4143-8529-824158b28430"}
